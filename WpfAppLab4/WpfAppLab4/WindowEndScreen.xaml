<Window x:Class="WpfAppLab4.WindowEndScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppLab4" d:DataContext="{d:DesignInstance Type=local:ApplyViewModelEndScreen}"
        mc:Ignorable="d"
        Title="WindowEndScreen"  Height="300" Width="300">
    <Grid>
        <Label Content="{Binding LabelText}" HorizontalAlignment="Center" Margin="0,27,0,0" VerticalAlignment="Top"/>
        <Button Content="Ок" HorizontalAlignment="Center" Margin="0,108,0,0" VerticalAlignment="Top" Click="Button_Click"/>
    </Grid>
</Window>
